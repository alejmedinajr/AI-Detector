class Solution:
    def mergeTwoLists(self, l1: ListNode, l2: ListNode) -> ListNode:
        stack = []
        while l1 and l2:
            if l1.val < l2.val:
                stack.append(l1)
                l1 = l1.next
            else:
                stack.append(l2)
                l2 = l2.next
        remaining = l1 or l2
        while remaining:
            stack.append(remaining)
            remaining = remaining.next
        head = tail = None
        while stack:
            node = stack.pop()
            node.next = tail
            tail = node
        return tail
