class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        count = {}
        left = 0
        max_length = 0
        for right, char in enumerate(s):
            count[char] = count.get(char, 0) + 1
            while count[char] > 1:
                count[s[left]] -= 1
                left += 1
            max_length = max(max_length, right - left + 1)
        return max_length
