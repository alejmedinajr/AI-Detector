class Solution:
   def lengthOfLongestSubstring(self, s: str) -> int:
       substring = []
       max_len = 0
       for i in s:
           if i in substring:
               j_idx = substring.index(i)
               substring = substring[j_idx+1:]
           substring.append(i)
           if len(substring) > max_len:
               max_len = len(substring)
           print(substring)
       return max_len